                        <Entry Grid.Row="1" Grid.Column="2" x:Name="H_D_11_AEntryField" Keyboard="Numeric" />
                        <Button Grid.Row="1" Grid.Column="3" Text="+" Clicked="IncreaseButton_Clicked2" Pressed="IncreaseButton_Pressed" Released="IncreaseButton_Released" />
