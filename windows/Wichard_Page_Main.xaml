                                            Grid.Row="2"
                                            Grid.Column="0"
                                            FontSize="16"
                                            Text="Выносливость" />
                        <Entry
                                            x:Name="EnduranceEntryField"
                                            Grid.Row="2"
                                            Grid.Column="1"
                                            Keyboard="Numeric"
                                            />
