                        <Entry Grid.Row="4" Grid.Column="2" x:Name="RH_D_31_AEntryField" Keyboard="Numeric" />
                        <Button Grid.Row="4" Grid.Column="3" Text="+" Clicked="IncreaseButton_Clicked2" Pressed="IncreaseButton_Pressed" Released="IncreaseButton_Released" />
                        <Entry Grid.Row="4" Grid.Column="4" x:Name="RH_PB_32_AEntryField" Keyboard="Numeric" Text="12" IsReadOnly="True"/>

