<?xml version="1.0" encoding="UTF-8" ?>
<ContentPage
    x:Class="MauiApp7.Wichard_Page_Main"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:toolkit="http://schemas.microsoft.com/dotnet/2022/maui/toolkit"
    xmlns:material="http://schemas.enisn-projects.io/dotnet/maui/uraniumui/material"
    xmlns:uranium="http://schemas.enisn-projects.io/dotnet/maui/uraniumui"
    xmlns:input="clr-namespace:InputKit.Shared.Controls;assembly=InputKit.Maui"
    xmlns:local="clr-namespace:MauiApp7"
    Title="Wichard Page Main">
    <ContentPage.Resources>
        <ResourceDictionary>
            <toolkit:MultiMathExpressionConverter x:Key="MultiMathExpressionConverter" />
            <toolkit:MathExpressionConverter x:Key="MathExpressionConverter" />
            <Style TargetType="Label">
                <Setter Property="TextColor" Value="{Binding Source={x:Reference ColorPicker}, Path=SelectedItem }" />
            </Style>
            <Style TargetType="Entry">
                <Setter Property="TextColor" Value="{Binding Source={x:Reference ColorPicker}, Path=SelectedItem, Converter={StaticResource ColorConverter}}" />
            </Style>
            <Style TargetType="Button">
                <Setter Property="TextColor" Value="{Binding Source={x:Reference ColorPicker}, Path=SelectedItem, Converter={StaticResource ColorConverter}}" />
            </Style>
            <Style TargetType="Editor">
                <Setter Property="TextColor" Value="{Binding Source={x:Reference ColorPicker}, Path=SelectedItem, Converter={StaticResource ColorConverter}}" />
            </Style>
        </ResourceDictionary>
    </ContentPage.Resources>




    <StackLayout>
        <StackLayout Orientation="Horizontal">
            <!-- Кнопка "+" -->
            <Button Text="+" Clicked="IncreaseButton_Clicked" />
            <!-- Кнопка "-" -->
            <Button Text="-" Clicked="DecreaseButton_Clicked" />
            <Button Text="Обновить" Clicked="RefreshButton_Clicked" />
            <Picker x:Name="ColorPicker" >
                <Picker.ItemsSource>
                    <x:Array Type="{x:Type x:String}">
                        <x:String>Black</x:String>
                        <x:String>White</x:String>
                        <x:String>Red</x:String>
                        <x:String>Green</x:String>
                        <x:String>Blue</x:String>
                    </x:Array>
                </Picker.ItemsSource>
            </Picker>
        </StackLayout>
        
        
        <StackLayout>
            <ScrollView
        HorizontalScrollBarVisibility="Always"
        Orientation="Both"
        VerticalOptions="FillAndExpand"
        VerticalScrollBarVisibility="Always"
        HeightRequest="600"
        x:Name="Main" >
                <Grid
                    Margin="2"
                    BackgroundColor="White"
                    ColumnSpacing="6"
                    HorizontalOptions="FillAndExpand"
                    VerticalOptions="CenterAndExpand"
                    RowSpacing="6">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="260" />
                        <ColumnDefinition Width="260" />
                        <ColumnDefinition Width="260" />
                        <ColumnDefinition Width="*" />
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="1*"/>
                        <RowDefinition Height="1*"/>
                        <RowDefinition Height="250"/>
                        <RowDefinition Height="1*"/>
                        <RowDefinition Height="1*"/>

                    </Grid.RowDefinitions>
                    
                    <Grid
                                x:Name="Main_box"
                                Grid.Row="0"
                                Grid.Column="0"
                                BackgroundColor="#ddd"
                                ColumnDefinitions="1*,Auto"
                                HorizontalOptions="FillAndExpand"
                                VerticalOptions="StartAndExpand"
                                RowDefinitions="1*,Auto,Auto,Auto,Auto"
                                >
                        <Label
                                            x:Name="name_lable"
                                            Grid.Row="0"
                                            Grid.Column="0"
                                            FontSize="16"
                                            Text="Имя" />
                        <Entry
                                            x:Name="nameEntryField"
                                            Grid.Row="0"
                                            Grid.Column="1"
                                            Placeholder="Имя" />
                        <Label
                                            x:Name="Job_lable"
                                            Grid.Row="1"
                                            Grid.Column="0"
                                            FontSize="16"
                                            Text="Класс (Профессия)" />
                        <Entry
                                            x:Name="JobEntryField"
                                            Grid.Row="1"
                                            Grid.Column="1"
                                            Keyboard="Default"
                                            Placeholder="Класс" />
                        <Label
                                            x:Name="Race_lable"
                                            Grid.Row="2"
                                            Grid.Column="0"
                                            FontSize="16"
                                            Text="Расса" />
                        <Entry
                                            x:Name="RaceEntryField"
                                            Grid.Row="2"
                                            Grid.Column="1"
                                            Keyboard="Default"
                                            Placeholder="Раса" />
                        <Label
                                            x:Name="Sex_lable"
                                            Grid.Row="3"
                                            Grid.Column="0"
                                            FontSize="16"
                                            Text="Пол" />
                        <Entry
                                            x:Name="SexEntryField"
                                            Grid.Row="3"
                                            Grid.Column="1"
                                            Keyboard="Default"
                                            Placeholder="Пол" />
                        <Label
                                            x:Name="Age_lable"
                                            Grid.Row="4"
                                            Grid.ColumnSpan="0"
                                            FontSize="16"
                                            Text="Возраст" />
                        <Entry
                                            x:Name="AgeEntryField"
                                            Grid.Row="4"
                                            Grid.Column="1"
                                            Keyboard="Default"
                                            Placeholder="Возраст" />
                    </Grid>
                    <!--Main_box-->
                    <Grid
                                x:Name="Main_skill_box"
                                Grid.Row="0"
                                Grid.Column="1"
                            ColumnDefinitions="1*,Auto"
                            RowDefinitions="1*,Auto,Auto,Auto,Auto,Auto,Auto,Auto"
                        HorizontalOptions="FillAndExpand"
                        VerticalOptions="StartAndExpand"
                                BackgroundColor="#ddd">
                        <Label
                                            x:Name="Intelligence_lable"
                                            Grid.Row="0"
                                            Grid.Column="0"
                                            FontSize="16"
                                            Text="Интелект" />
                        <Entry
                                            x:Name="IntelligenceEntryField"
                                            Grid.Row="0"
                                            Grid.Column="1"
                                            Keyboard="Numeric" />
                        <Label
                                            x:Name="Reflexes_lable"
                                            Grid.Row="1"
                                            Grid.Column="0"
                                            FontSize="16"
                                            Text="Рекация (Рефлексы)" />
                        <Entry
                                            x:Name="ReflexesEntryField"
                                            Grid.Row="1"
                                            Grid.Column="1"
                                            Keyboard="Numeric" />
                        <Label
                                            x:Name="Dexterity_lable"
                                            Grid.Row="2"
                                            Grid.Column="0"
                                            FontSize="16"
                                            Text="Ловкость" />
                        <Entry
                                            x:Name="DexterityEntryField"
                                            Grid.Row="2"
                                            Grid.Column="1"
                                            Keyboard="Numeric" />
                        <Label
                                            x:Name="Body_lable"
                                            Grid.Row="3"
                                            Grid.ColumnSpan="0"
                                            FontSize="16"
                                            Text="Тело" />
                        <Entry
                                            x:Name="BodyEntryField"
                                            Grid.Row="3"
                                            Grid.Column="1"
                                            TextChanged="Weight_Calculation"
                                            Keyboard="Numeric" />
                        <Label
                                            x:Name="Speed_lable"
                                            Grid.Row="4"
                                            Grid.Column="0"
                                            FontSize="16"
                                            Text="Скорость" />
                        <Entry
                                            x:Name="SpeedEntryField"
                                            Grid.Row="4"
                                            Grid.Column="1"
                                            Keyboard="Numeric" />
                        <Label
                                            x:Name="Empathy_lable"
                                            Grid.Row="5"
                                            Grid.Column="0"
                                            FontSize="16"
                                            Text="Эмпатия" />
                        <Entry
                                            x:Name="EmpathyEntryField"
                                            Grid.Row="5"
                                            Grid.Column="1"
                                            Keyboard="Numeric" />
                        <Label
                                            x:Name="Repair_lable"
                                            Grid.Row="6"
                                            Grid.Column="0"
                                            FontSize="16"
                                            Text="Ремонт(Изготовление)" />
                        <Entry
                                            x:Name="RepairEntryField"
                                            Grid.Row="6"
                                            Grid.Column="1"
                                            Keyboard="Numeric" />
                        <Label
                                            x:Name="Will_lable"
                                            Grid.Row="7"
                                            Grid.ColumnSpan="0"
                                            FontSize="16"
                                            Text="Воля" />
                        <Entry
                                            x:Name="WillEntryField"
                                            Grid.Row="7"
                                            Grid.Column="1"
                                            Keyboard="Numeric" />
                    </Grid>
                    <!--Main_skill_box-->
                    <Grid
                                x:Name="Second_skill_box"
                                Grid.Row="0"
                                Grid.Column="2"
                                BackgroundColor="#ddd"
                        HorizontalOptions="FillAndExpand"
                        VerticalOptions="StartAndExpand"
                                RowDefinitions="1*,Auto,Auto,Auto,Auto,Auto,Auto,Auto"
                                ColumnDefinitions="1*,Auto"
                                >
                        <Label
                                            x:Name="Rest_lable"
                                            Grid.Row="0"
                                            Grid.Column="0"
                                            FontSize="16"
                                            Text="Отдых" />
                        <Entry
                                            x:Name="restEntryField"
                                            Grid.Row="0"
                                            Grid.Column="1"
                                            Keyboard="Numeric" />
                        <Label
                                            x:Name="Weight_lable"
                                            Grid.Row="1"
                                            Grid.Column="0"
                                            FontSize="16"
                                            Text="Вес" />
                        <Entry
                                            x:Name="WeightEntryField"
                                            Grid.Row="1"
                                            Grid.Column="1"
                                            Keyboard="Numeric" 
