                        <Entry Grid.Row="5" Grid.Column="2" x:Name="LL_D_14_AEntryField" Keyboard="Numeric" />
                        <Button Grid.Row="5" Grid.Column="3" Text="+" Clicked="IncreaseButton_Clicked2" Pressed="IncreaseButton_Pressed" Released="IncreaseButton_Released" />
