                        <Entry Grid.Row="2" Grid.Column="2" x:Name="B_D_21_AEntryField" Keyboard="Numeric" />
                        <Button Grid.Row="2" Grid.Column="3" Text="+" Clicked="IncreaseButton_Clicked2" Pressed="IncreaseButton_Pressed" Released="IncreaseButton_Released" />
                        <Entry Grid.Row="2" Grid.Column="4" x:Name="B_PB_22_AEntryField" Keyboard="Numeric" Text="12" IsReadOnly="True"/>

                        <Label Grid.Row="3" Grid.Column="0"  HorizontalOptions="Center" x:Name="L_Hand_lable" FontSize="16" Text="Левая Рука" />
                        <Button Grid.Row="3" Grid.Column="1" Text="-" Clicked="DecreaseButton_Clicked2" Pressed="DecreaseButton_Pressed" Released="DecreaseButton_Released" />
                        <Entry Grid.Row="3" Grid.Column="2" x:Name="LH_D_34_AEntryField" Keyboard="Numeric" />
                        <Button Grid.Row="3" Grid.Column="3" Text="+" Clicked="IncreaseButton_Clicked2" Pressed="IncreaseButton_Pressed" Released="IncreaseButton_Released" />
                        <Entry Grid.Row="3" Grid.Column="4" x:Name="LH_PB_35_AEntryField" Keyboard="Numeric" Text="12" IsReadOnly="True"/>

                        <Label Grid.Row="4" Grid.Column="0"  HorizontalOptions="Center" x:Name="R_Hand_lable" FontSize="16" Text="Правая Рука" />
