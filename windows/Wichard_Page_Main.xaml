                                            Grid.ColumnSpan="0"
                                            FontSize="16"
                                            Text="ПЗ" />
                        <Entry
                                            x:Name="HPEntryField"
                                            Grid.Row="3"
                                            Grid.Column="1"
                                            Keyboard="Numeric" 
                                            />
                        <Label
