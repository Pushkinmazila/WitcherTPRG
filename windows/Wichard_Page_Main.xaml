                        <Label Grid.Row="6" Grid.Column="0"  HorizontalOptions="Center" x:Name="R_Leg_lable" FontSize="16" Text="Правая Нога" />
                        <Button Grid.Row="6" Grid.Column="1" Text="-" Clicked="DecreaseButton_Clicked2" Pressed="DecreaseButton_Pressed" Released="DecreaseButton_Released" />
